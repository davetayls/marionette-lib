{
  "name": "marionette_lib",
  "version": "3.2.9",
  "homepage": "https://github.com/davetayls/marionette_lib",
  "authors": [
    "davetayls <dave@the-taylors.org>"
  ],
  "main": [
    "amd/**/*.js",
    "amd/**/*.js.map",
    "amd/**/*.d.ts",
    "amd/**/*.hbs",
    "amd/**/*.json",
    "src/**/*.less",
    "src/**/*.css"
  ],
  "description": "A reusable library for Marionette apps",
  "moduleType": [
    "amd"
  ],
  "keywords": [
    "marionette"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "bower_components",
    "grunt",
    "Gruntfile.*",
    "karma.conf.js",
    "node_modules",
    "src/**/*.ts",
    "src/**/*.coffee",
    "test",
    "test-client",
    "test-main.js",
    "tests"
  ],
  "dependencies": {
    "async": "~0.9.0",
    "backbone": "~1.1.2",
    "backbone.marionette": "~2.0.1",
    "fastclick": "~1.0.2",
    "handlebars": "~1.3.0",
    "i18next": "~1.7.3",
    "modernizr": "~2.8.2",
    "requirejs": "~2.1.14",
    "requirejs-hbs": "~0.1.1",
    "requirejs-plugins": "~1.0.2",
    "requirejs-text": "~2.0.12",
    "showdown": "~0.3.1",
    "snapjs": "~1.9.3",
    "spin.js": "2.x.x",
    "flux": "~2.0.2",
    "q": "1.x.x"
  }
}
